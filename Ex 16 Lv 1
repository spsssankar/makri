//PF-Exer-16

num=4
num1=5
num2=10
flag=0

//Write your code here
while(flag == 0){
    if (num%num1==0){
        if(num%num2==0){
            flag=1
        }
    }
    else if(num%num2==0){
        if (num%num1==0){
            flag=1
        }
    }
    else{
        num=num+1
    }
}
console.log(num)


